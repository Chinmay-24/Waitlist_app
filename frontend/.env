# ============================
# FRONTEND ENVIRONMENT CONFIGURATION
# ============================

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# Environment
REACT_APP_ENV=development

# Security
REACT_APP_TOKEN_STORAGE=secure-storage
REACT_APP_ENABLE_REQUEST_LOGGING=false

# Feature Flags
REACT_APP_ENABLE_MAP_VIEW=true
REACT_APP_ENABLE_CHECKOUT=true
REACT_APP_ENABLE_REVIEWS=true
REACT_APP_ENABLE_CUISINE_FILTER=true

# Session
REACT_APP_SESSION_TIMEOUT=3600000
